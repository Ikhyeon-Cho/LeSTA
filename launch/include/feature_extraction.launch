<launch>

  <!-- Terrain Feature Extraction -->
  <node pkg="traversability_learning" type="feature_extraction_node" name="feature_extraction" output="screen">
    <rosparam file="$(find traversability_learning)/config/terrain_features/params.yaml" command="load" />
  </node>

  <!-- Feature Map Visualization -->
  <node pkg="grid_map_visualization" type="grid_map_visualization" name="feature_visualization" output="log">
    <rosparam file="$(find traversability_learning)/config/terrain_features/visualization.yaml" />
  </node>

</launch>